	string requiredMark;
	public string RequiredMark {
		get => isInitialized ? DxFormInterop.GetRequiredMark(Id) : requiredMark;
		set {
			if (isInitialized) {
				DxFormInterop.SetRequiredMark(Id, value);
			} else {
				this.requiredMark = value;
			}
		}
	}
	string requiredMessage;
