@implements IDisposable
@inherits DxBlazorCompoment<DevExpress.Ui.DxTabsOptions>

<div id="@Id"></div>

@functions {
	public DxTabs() : base("DxTabs") { }
	protected override DevExpress.Ui.DxTabsOptions CreateOptions() {
		return new DevExpress.Ui.DxTabsOptions {
			focusStateEnabled = focusStateEnabled,
			hoverStateEnabled = hoverStateEnabled,
			scrollByContent = scrollByContent,
			scrollingEnabled = scrollingEnabled,
			selectedItems = selectedItems,
			selectionMode = selectionMode,
			showNavButtons = showNavButtons,
		};
	}
	protected override void Init(DevExpress.Ui.DxTabsOptions options) {
		DevExpress.Ui.DxTabsInterop.Init(Id, options);
	}
	bool? focusStateEnabled;
	public bool? FocusStateEnabled {
		get => isInitialized ? DevExpress.Ui.DxTabsInterop.GetFocusStateEnabled(Id) : focusStateEnabled;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxTabsInterop.SetFocusStateEnabled(Id, value);
			} else {
				this.focusStateEnabled = value;
			}
		}
	}
	bool? hoverStateEnabled;
	public bool? HoverStateEnabled {
		get => isInitialized ? DevExpress.Ui.DxTabsInterop.GetHoverStateEnabled(Id) : hoverStateEnabled;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxTabsInterop.SetHoverStateEnabled(Id, value);
			} else {
				this.hoverStateEnabled = value;
			}
		}
	}
	bool? scrollByContent;
	public bool? ScrollByContent {
		get => isInitialized ? DevExpress.Ui.DxTabsInterop.GetScrollByContent(Id) : scrollByContent;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxTabsInterop.SetScrollByContent(Id, value);
			} else {
				this.scrollByContent = value;
			}
		}
	}
	bool? scrollingEnabled;
	public bool? ScrollingEnabled {
		get => isInitialized ? DevExpress.Ui.DxTabsInterop.GetScrollingEnabled(Id) : scrollingEnabled;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxTabsInterop.SetScrollingEnabled(Id, value);
			} else {
				this.scrollingEnabled = value;
			}
		}
	}
	object selectedItems;
	public object SelectedItems {
		get => isInitialized ? DevExpress.Ui.DxTabsInterop.GetSelectedItems(Id) : selectedItems;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxTabsInterop.SetSelectedItems(Id, value);
			} else {
				this.selectedItems = value;
			}
		}
	}
	string selectionMode;
	public string SelectionMode {
		get => isInitialized ? DevExpress.Ui.DxTabsInterop.GetSelectionMode(Id) : selectionMode;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxTabsInterop.SetSelectionMode(Id, value);
			} else {
				this.selectionMode = value;
			}
		}
	}
	bool? showNavButtons;
	public bool? ShowNavButtons {
		get => isInitialized ? DevExpress.Ui.DxTabsInterop.GetShowNavButtons(Id) : showNavButtons;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxTabsInterop.SetShowNavButtons(Id, value);
			} else {
				this.showNavButtons = value;
			}
		}
	}
	protected override async Task OnInitAsync() {
		await base.OnInitAsync();
	}
	public void Dispose() {
	}
}
