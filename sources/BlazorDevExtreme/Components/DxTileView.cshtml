		set {
			if (isInitialized) {
				DevExpress.Ui.DxTileViewInterop.SetDirection(Id, value);
			} else {
				this.direction = value;
			}
		}
	}
	bool? focusStateEnabled;
	public bool? FocusStateEnabled {
