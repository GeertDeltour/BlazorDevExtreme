@implements IDisposable
@inherits DxBlazorCompoment<DevExpress.Ui.DxPanoramaOptions>

<div id="@Id"></div>

@functions {
	public DxPanorama() : base("DxPanorama") { }
	protected override DevExpress.Ui.DxPanoramaOptions CreateOptions() {
		return new DevExpress.Ui.DxPanoramaOptions {
			backgroundImage = backgroundImage,
			focusStateEnabled = focusStateEnabled,
			selectedIndex = selectedIndex,
			title = title,
		};
	}
	protected override void Init(DevExpress.Ui.DxPanoramaOptions options) {
		DevExpress.Ui.DxPanoramaInterop.Init(Id, options);
	}
	object backgroundImage;
	public object BackgroundImage {
		get => isInitialized ? DevExpress.Ui.DxPanoramaInterop.GetBackgroundImage(Id) : backgroundImage;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxPanoramaInterop.SetBackgroundImage(Id, value);
			} else {
				this.backgroundImage = value;
			}
		}
	}
	bool? focusStateEnabled;
	public bool? FocusStateEnabled {
		get => isInitialized ? DevExpress.Ui.DxPanoramaInterop.GetFocusStateEnabled(Id) : focusStateEnabled;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxPanoramaInterop.SetFocusStateEnabled(Id, value);
			} else {
				this.focusStateEnabled = value;
			}
		}
	}
	float? selectedIndex;
	public float? SelectedIndex {
		get => isInitialized ? DevExpress.Ui.DxPanoramaInterop.GetSelectedIndex(Id) : selectedIndex;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxPanoramaInterop.SetSelectedIndex(Id, value);
			} else {
				this.selectedIndex = value;
			}
		}
	}
	string title;
	public string Title {
		get => isInitialized ? DevExpress.Ui.DxPanoramaInterop.GetTitle(Id) : title;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxPanoramaInterop.SetTitle(Id, value);
			} else {
				this.title = value;
			}
		}
	}
	protected override async Task OnInitAsync() {
		await base.OnInitAsync();
	}
	public void Dispose() {
	}
}
