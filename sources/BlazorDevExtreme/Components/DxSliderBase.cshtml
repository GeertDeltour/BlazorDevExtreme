@implements IDisposable
@inherits DxBlazorCompoment<DevExpress.Ui.DxSliderBaseOptions>

<div id="@Id"></div>

@functions {
	public DxSliderBase() : base("DxSliderBase") { }
	protected override DevExpress.Ui.DxSliderBaseOptions CreateOptions() {
		return new DevExpress.Ui.DxSliderBaseOptions {
			activeStateEnabled = activeStateEnabled,
			focusStateEnabled = focusStateEnabled,
			hoverStateEnabled = hoverStateEnabled,
			keyStep = keyStep,
			label = label,
			name = name,
			showRange = showRange,
			step = step,
			tooltip = tooltip,
		};
	}
	protected override void Init(DevExpress.Ui.DxSliderBaseOptions options) {
		DevExpress.Ui.DxSliderBaseInterop.Init(Id, options);
	}
	bool? activeStateEnabled;
	public bool? ActiveStateEnabled {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetActiveStateEnabled(Id) : activeStateEnabled;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetActiveStateEnabled(Id, value);
			} else {
				this.activeStateEnabled = value;
			}
		}
	}
	bool? focusStateEnabled;
	public bool? FocusStateEnabled {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetFocusStateEnabled(Id) : focusStateEnabled;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetFocusStateEnabled(Id, value);
			} else {
				this.focusStateEnabled = value;
			}
		}
	}
	bool? hoverStateEnabled;
	public bool? HoverStateEnabled {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetHoverStateEnabled(Id) : hoverStateEnabled;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetHoverStateEnabled(Id, value);
			} else {
				this.hoverStateEnabled = value;
			}
		}
	}
	float? keyStep;
	public float? KeyStep {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetKeyStep(Id) : keyStep;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetKeyStep(Id, value);
			} else {
				this.keyStep = value;
			}
		}
	}
	object label;
	public object Label {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetLabel(Id) : label;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetLabel(Id, value);
			} else {
				this.label = value;
			}
		}
	}
	string name;
	public string Name {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetName(Id) : name;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetName(Id, value);
			} else {
				this.name = value;
			}
		}
	}
	bool? showRange;
	public bool? ShowRange {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetShowRange(Id) : showRange;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetShowRange(Id, value);
			} else {
				this.showRange = value;
			}
		}
	}
	float? step;
	public float? Step {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetStep(Id) : step;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetStep(Id, value);
			} else {
				this.step = value;
			}
		}
	}
	object tooltip;
	public object Tooltip {
		get => isInitialized ? DevExpress.Ui.DxSliderBaseInterop.GetTooltip(Id) : tooltip;
		set {
			if (isInitialized) {
				DevExpress.Ui.DxSliderBaseInterop.SetTooltip(Id, value);
			} else {
				this.tooltip = value;
			}
		}
	}
	protected override async Task OnInitAsync() {
		await base.OnInitAsync();
	}
	public void Dispose() {
	}
}
