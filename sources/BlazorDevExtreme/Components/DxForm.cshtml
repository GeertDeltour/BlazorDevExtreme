		}
	}
	bool? showOptionalMark;
	public bool? ShowOptionalMark {
		get => isInitialized ? DxFormInterop.GetShowOptionalMark(Id) : showOptionalMark;
		set {
			if (isInitialized) {
				DxFormInterop.SetShowOptionalMark(Id, value);
			} else {
				this.showOptionalMark = value;
			}
		}
	}
